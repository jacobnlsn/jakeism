<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="BusinessLayer"
                   namespace="BusinessLayer.Domain">

  <class name="Entry" table="JAKEISM_ENTRY">
    
    <id name="Id" column="ENTRY_ID">
      <generator class="increment" />
    </id>
    
    <property name="EntryBody" />
    <property name="Date" type="DateTime" />
    
    <many-to-one name="User" class="User" column="USER_ID" cascade="none" />
    
    <set name="Votes" table="JAKEISM_VOTES" cascade="none"  >
      <key column="ENTRY_ID" />
      <many-to-many class="User" column="USER_ID" />
    </set>
    
  </class>

</hibernate-mapping>