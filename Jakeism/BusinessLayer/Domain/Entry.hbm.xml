<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="BusinessLayer"
                   namespace="BusinessLayer.Domain">

  <class name="Entry" table="JAKEISM_ENTRY">
    <id name="Id" column="ENTRY_ID" type="Guid">
      <generator class="guid" />
    </id>
    <property name="EntryBody" />
    <many-to-one name="Usr" class="User" column="USER_ID" cascade="none" />
    <property name="Date" type="DateTime" />
    <set name="Votes" table="JAKEISM_VOTES" cascade="none"  >
      <key column="ENTRY_ID" />
      <many-to-many class="User" column="USER_ID" />
    </set>
  </class>

</hibernate-mapping>